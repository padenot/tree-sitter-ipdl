{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ipdl",
      "camelcase": "ipdl",
      "title": "ipdl",
      "scope": "ipdl",
      "file-types": [
        "ipdl"
      ],
      "injection-regex": "^ipdl$",
      "class-name": "TreeSitterIpdl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MPL-2",
    "description": "Parser for Mozilla's IPDL (Inter-(thread|process) Protocol Definition Language",
    "authors": [
      {
        "name": "Paul Adenot",
        "email": "padenot@mozilla.com",
        "url": "https://paul.cx/"
      }
    ],
    "links": {
      "repository": "https://github.com/padenot/tree-sitter-ipdl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
